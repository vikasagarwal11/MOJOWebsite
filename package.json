{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "dev:network": "vite --host 0.0.0.0 --port 5173 --mode development",
    "build": "vite build --mode production",
    "build:dev": "vite build --mode development",
    "build:staging": "vite build --mode staging",
    "build:prod": "vite build --mode production",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "deploy:dev": "npm run build:dev && firebase deploy --project=dev",
    "deploy:dev:hosting": "npm run build:dev && firebase deploy --only hosting --project=dev",
    "deploy:dev:firestore": "firebase deploy --only firestore --project=dev",
    "deploy:dev:functions": "firebase deploy --only functions --project=dev",
    "deploy:staging": "npm run build:staging && firebase deploy --project=staging --config=firebase.staging.json",
    "deploy:staging:hosting": "npm run build:staging && firebase deploy --only hosting --project=staging --config=firebase.staging.json",
    "deploy:staging:firestore": "firebase deploy --only firestore --project=staging --config=firebase.staging.json",
    "deploy:staging:functions": "firebase deploy --only functions --project=staging --config=firebase.staging.json",
    "deploy:prod": "npm run build:prod && firebase deploy --project=prod --config=firebase.prod.json",
    "deploy:prod:hosting": "npm run build:prod && firebase deploy --only hosting --project=prod --config=firebase.prod.json",
    "deploy:prod:firestore": "firebase deploy --only firestore --project=prod --config=firebase.prod.json",
    "deploy:prod:functions": "firebase deploy --only functions --project=prod --config=firebase.prod.json",
    "emulators": "firebase emulators:start --project=dev"
  },
  "dependencies": {
    "@ffprobe-installer/ffprobe": "^2.1.2",
    "@firebasegen/default-connector": "file:dataconnect-generated/js/default-connector",
    "@hookform/resolvers": "^5.2.1",
    "@types/qrcode": "^1.5.5",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^10.0.0",
    "@zxing/library": "^0.21.3",
    "date-fns": "^4.1.0",
    "exifr": "^7.1.3",
    "ffmpeg-static": "^5.2.0",
    "firebase": "^11.10.0",
    "firebase-tools": "^14.14.0",
    "fluent-ffmpeg": "^2.1.3",
    "framer-motion": "^12.23.12",
    "hls.js": "^1.6.10",
    "ical": "^0.8.0",
    "ics": "^3.8.1",
    "lucide-react": "^0.344.0",
    "qrcode": "^1.5.4",
    "react": "^18.3.1",
    "react-big-calendar": "^1.19.4",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.62.0",
    "react-hot-toast": "^2.6.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-intersection-observer": "^9.16.0",
    "react-popper-tooltip": "^4.4.2",
    "react-qr-scanner": "^1.0.0-alpha.11",
    "react-router-dom": "^7.8.1",
    "react-window": "^1.8.11",
    "react-window-infinite-loader": "^1.0.10",
    "react-zoom-pan-pinch": "^3.7.0",
    "rrule": "^2.8.1",
    "sharp": "^0.34.3",
    "use-debounce": "^10.0.5",
    "uuid": "^11.1.0",
    "workbox-window": "^7.3.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.5",
    "@types/react-big-calendar": "^1.16.2",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vite-plugin-pwa": "^0.20.5",
    "vitest": "^3.2.4"
  }
}
